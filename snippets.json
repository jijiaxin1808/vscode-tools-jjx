{
	"reg@chinese": {
		"prefix": "reg@chinese",
		"body": [
			"/[\\u4e00-\\u9fa5]/gm"
		],
		"description": "匹配中文的正则表达式"
	},
	"reg@email": {
		"prefix": "reg@email",
		"body": [
			"/\\w+([-+.]\\w+)*@\\w+([-.]\\w+)*\\.\\w+([-.]\\w+)*/"
		],
		"description": "匹配邮箱的正则表达式"
	},
	"reg@phone": {
		"prefix": "reg@phone",
		"body": [
			"/^(0|86|17951)?(13[0-9]|15[012356789]|166|17[3678]|18[0-9]|14[57])[0-9]{8}$/"
		],
		"description": "匹配手机号的正则表达式"
	},
	"reg@id": {
		"prefix": "reg@id",
		"body": [
			"/^(^[1-9]\\d{7}((0\\d)|(1[0-2]))(([0|1|2]\\d)|3[0-1])\\d{3}$)|(^[1-9]\\d{5}[1-9]\\d{3}((0\\d)|(1[0-2]))(([0|1|2]\\d)|3[0-1])((\\d{4})|\\d{3}[Xx])$)$/"
		],
		"description": "匹配身份证号的正则表达式"
	}
}